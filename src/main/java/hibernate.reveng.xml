<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="INTEGER" hibernate-type="int"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"
			length="200" not-null="true">
		</sql-type>
		<sql-type jdbc-type="TIMESTAMP" hibernate-type="date"
			length="19" not-null="true">
		</sql-type>
		<sql-type jdbc-type="TINYINT" hibernate-type="int"
			not-null="true">
		</sql-type>
	</type-mapping>

	<table-filter match-catalog="Portal" match-name="User" />
	<table-filter match-name="Announce" match-catalog="Portal"></table-filter>
	<table-filter match-name="Document" match-catalog="Portal"></table-filter>
	<table-filter match-name="Image" match-catalog="Portal"></table-filter>
</hibernate-reverse-engineering>